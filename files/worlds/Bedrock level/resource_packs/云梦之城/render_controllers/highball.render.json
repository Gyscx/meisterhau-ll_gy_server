{
    "format_version": "1.10.0",
    "render_controllers": {
        "controller.render.highball": {
            "arrays": {
                "textures": {
                    "Array.textures": [
                        "texture.default",
                        "texture.vodka",
                        "texture.whiskey",
                        "texture.wine"
                    ]
                },
                "geometries": {
                    "Array.geometries": [
                        "geometry.default",
                        "geometry.full",
                        "geometry.iced",
                        "geometry.iced_full"
                    ]
                }
            },
            "geometry": "Array.geometries[v.full + v.iced * 2]",
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "Array.textures[v.type]"
            ]
        }
    }
}