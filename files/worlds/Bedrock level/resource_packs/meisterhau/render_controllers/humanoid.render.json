{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.player.third_person": {
            "arrays": {
                "geometries": {
                    "Array.geos": [
                        "Geometry.steve",
                        "Geometry.alex"
                    ]
                }
            },
            "geometry": "Array.geos[v.is_slim_arm]",
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "texture.default"
            ],
            "part_visibility": [
                {
                    "*": true,
                    "*Sleeve": false
                }
            ],
            "overlay_color": {
                "r": "v.overlay_color.r",
                "g": "v.overlay_color.g",
                "b": "v.overlay_color.b",
                "a": "v.overlay_color.a"
            }
        },
        "controller.render.player.map": {
            "geometry": "Geometry.alex",
            "materials": [
                {
                    "*": "Material.default"
                }
            ],
            "textures": [
                "Texture.default"
            ],
            "part_visibility": [
                {
                    "*": false
                },
                {
                    "head": true
                },
                {
                    "hat": true
                },
                {
                    "helmet": true
                }
            ]
        },
        "controller.render.player.arm_overlay": {
            "arrays": {
                "textures": {
                    "Array.t": [
                        "texture.default",
                        "texture.unstoppable_steve",
                        "texture.trace_steve",
                        "texture.block_steve",
                        "texture.unstoppable",
                        "texture.trace",
                        "texture.block"
                    ]
                },
                "geometries": {
                    "Array.geos": [
                        "Geometry.steve",
                        "Geometry.alex"
                    ]
                }
            },
            "geometry": "Array.geos[v.is_slim_arm]",
            "materials": [
                {
                    "*": "material.blend"
                }
            ],
            "textures": [
                "Array.t[v.arm_status == 0 ? 0 : v.arm_status + v.is_slim_arm * 3]"
            ],
            "part_visibility": [
                {
                    "*Sleeve": true
                }
            ]
        },
        "controller.render.guard.third_person": {
            "arrays": {
                "geometries": {
                    "Array.geos": [
                        "Geometry.steve",
                        "Geometry.alex"
                    ]
                }
            },
            "geometry": "Array.geos[v.is_slim_arm]",
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "texture.default"
            ],
            "part_visibility": [
                {
                    "*": true,
                    "weapon*": false
                }
            ],
            "overlay_color": {
                "r": "v.overlay_color.r",
                "g": "v.overlay_color.g",
                "b": "v.overlay_color.b",
                "a": "v.overlay_color.a"
            }
        },
        "controller.render.weapon.third_person": {
            "geometry": "Geometry.steve",
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "texture.weapon"
            ],
            "part_visibility": [
                {
                    "*": false,
                    "weapon*": true
                }
            ]
        },
        "controller.render.fake_renderer": {
            "geometry": "Geometry.default",
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "texture.default"
            ],
            "part_visibility": [
                {
                    "*": false,
                    "*Item": true
                }
            ]
        }
    }
}